{
  "name": "interview.dog",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android --active-arch-only",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "patch-package",
    "format": "biome format --write",
    "check": "biome check --write",
    "make-dir-android": "node -e \"require('fs').mkdirSync('android/output', { recursive: true })\"",
    "make-dir-ios": "node -e \"require('fs').mkdirSync('ios/output', { recursive: true })\"",
    "export-android-win": "yarn run make-dir-android && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/output/index.android.bundle --assets-dest android/output --sourcemap-output android/sourcemap.js && cd android && cross-zip output index.android.bundle.zip && cross-zip sourcemap.js sourcemap.zip && cd .. && rimraf android/output android/sourcemap.js",
    "export-android-unix": "yarn run make-dir-android && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/output/index.android.bundle --assets-dest android/output --sourcemap-output android/sourcemap.js && cd android && find output -type f | zip index.android.bundle.zip -@ && zip sourcemap.zip sourcemap.js && cd .. && rm -rf android/output && rm -rf android/sourcemap.js",
    "export-ios-win": "yarn run make-dir-ios && react-native bundle --platform ios --dev false  --entry-file index.js --bundle-output ios/output/main.jsbundle --assets-dest ios/output --sourcemap-output ios/sourcemap.js && cd ios && cross-zip output main.jsbundle.zip && cross-zip sourcemap.js sourcemap.zip && cd .. && rimraf ios/output ios/sourcemap.js",
    "export-ios-unix": "yarn run make-dir-ios && react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/output/main.jsbundle --assets-dest ios/output --sourcemap-output ios/sourcemap.js && cd ios && find output -type f | zip main.jsbundle.zip -@ && zip sourcemap.zip sourcemap.js && cd .. && rm -rf ios/output && rm -rf ios/sourcemap.js"
  },
